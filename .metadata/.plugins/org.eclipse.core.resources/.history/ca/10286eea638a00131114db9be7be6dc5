
public class Peiord {

	private static int pero(int s){
		String b = Integer.toBinaryString(s);
		int p=-1;
		System.out.println(s);
		int mid = b.length()/2;
		while(mid-1/2>0){
			mid--;
			if(b.substring(0,mid)== b.substring(mid,mid) ) break;
			p = mid;
			System.out.println("mid "+mid );
		}
		
		System.out.println("p "+p );
		
		return p;
	}
	
}
