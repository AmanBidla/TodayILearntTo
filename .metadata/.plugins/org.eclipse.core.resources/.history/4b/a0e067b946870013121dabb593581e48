import java.util.Comparator;
import java.util.PriorityQueue;
import java.util.Scanner;


public class RocketFuel {

	static class Input {
		
		int i;
		public Input(int i) {
			super();
			this.i = i;
		}
		
		public Input(){
			
		}
		
	}
	
	static class LessInput  implements Comparable<LessInput> {
		
		  
		 private Input im;
		 
		 public LessInput(Input i){
			 this.im=i;
					 
		 }

		@Override
		public int compareTo(LessInput o) {
			if(im.i<o.im.i) return 1;
			else if(im.i>o.im.i) return -1;
			return 0;
		}
		
		 
 	}
	
	static class MoreInput implements Comparable<MoreInput>{
		
		private Input im;
		
		public MoreInput(Input i){
			this.im = i;
		}
		

		@Override
		public int compareTo(MoreInput o) {
			if(im.i<o.im.i) return -1;
			else if(im.i>o.im.i) return 1;
			return 0;
		}
	}
	
	private static PriorityQueue<LessInput> pqLess = new PriorityQueue<LessInput>();
	private static PriorityQueue<MoreInput> pqMore = new PriorityQueue<MoreInput>(); 
	
	
	public static void main(String[] args) {
		 
		
		Input i;
		int a =-1,b=-1;
		Scanner in = new Scanner(System.in);
		String st = in.nextLine();
		a = Integer.parseInt(st);
		in = new Scanner(System.in);
		st = in.nextLine();
		b= Integer.parseInt(st);
		if(a<b) {
			pqMore.add(new MoreInput(new Input(a)));
			pqLess.add(new LessInput(new Input(b)));
		}else{
			pqMore.add(new MoreInput(new Input(b)));
			pqLess.add(new LessInput(new Input(a)));
		}
		while(true){
			in = new Scanner(System.in);
			st = in.nextLine();
			
			a = Integer.parseInt(st);
			if(a<pqMore.peek().im.i)
				pqMore.add(new MoreInput(new Input(a)));
			else
				pqLess.add(new LessInput(new Input(a)));
			
			if(pqMore.size()>pqLess.size()){
				swap(pqMore.poll(),pqLess.poll());
			}else{
				swap(pqLess.poll(),pqMore.poll());
			}
			
			if(pqMore.size()==pqLess.size())
				System.out.println((pqMore.peek().im.i+pqLess.peek().im.i)/2);
			
		}
		
		 
		
		System.out.println(pqLess.peek().im.i);
		System.out.println(pqMore.peek().im.i);
		 
	}
	
	
	
	
}
