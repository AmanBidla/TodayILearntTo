import java.util.Comparator;
import java.util.PriorityQueue;
import java.util.Scanner;


public class RocketFuel {

	static class Input {
		
		int i;
		public Input(int i) {
			super();
			this.i = i;
		}
		
		public Input(){
			
		}
		
	}
	
	static class LessInput  implements Comparable<LessInput> {
		
		  
		 private Input im;
		 
		 public LessInput(Input i){
			 this.im=i;
					 
		 }

		@Override
		public int compareTo(LessInput o) {
			if(im.i<o.im.i) return 1;
			else if(im.i>o.im.i) return -1;
			return 0;
		}
		
		 
 	}
	
	static class MoreInput implements Comparable<MoreInput>{
		
		private Input im;
		
		public MoreInput(Input i){
			this.im = i;
		}
		

		@Override
		public int compareTo(MoreInput o) {
			if(im.i<o.im.i) return -1;
			else if(im.i>o.im.i) return 1;
			return 0;
		}
	}
	
	private static PriorityQueue<LessInput> pqMore = new PriorityQueue<LessInput>();
	private static PriorityQueue<MoreInput> pqLess = new PriorityQueue<MoreInput>(); 
	
	
	public static void main(String[] args) {
		 
		
		Input i;
		int a =-1,b=-1;
		Scanner in = new Scanner(System.in);
		String st = in.nextLine();
		a = Integer.parseInt(st);
		in = new Scanner(System.in);
		st = in.nextLine();
		b= Integer.parseInt(st);
		if(a<b) {
			pqLess.add(new MoreInput(new Input(a)));
			pqMore.add(new LessInput(new Input(b)));
		}else{
			pqLess.add(new MoreInput(new Input(b)));
			pqMore.add(new LessInput(new Input(a)));
		}
		while(true){
			in = new Scanner(System.in);
			st = in.nextLine();
			
			a = Integer.parseInt(st);
			if(a<pqLess.peek().im.i)
				pqLess.add(new MoreInput(new Input(a)));
			else
				pqMore.add(new LessInput(new Input(a)));
			
			if(pqLess.size()>pqMore.size()){
				swap(pqLess.poll(),pqMore.poll());
			}else{
				swap(pqMore.poll(),pqLess.poll());
			}
			
			if(pqMore.size()==pqLess.size())
				System.out.println((pqMore.peek().im.i+pqLess.peek().im.i)/2);
			else{
				if(pqLess.size()>pqMore.size())
					System.out.println(pqLess.peek().im.i);
				else
					System.out.println(pqMore.peek().im.i);
			}
				
			
		}
		
		 
		 
		 
	}


	private static void swap(LessInput less, MoreInput more) {
		Input imore = new Input(less.im.i);
		pqMore.add(new MoreInput(imore));
		
		Input iless = new Input(more.im.i);
		pqLess.add(new LessInput(iless));
		
	}


	private static void swap(MoreInput more, LessInput less) {
		Input imore = new Input(more.im.i);
		pqLess.add(new LessInput(imore));
		
		Input iless = new Input(less.im.i);
		pqMore.add(new MoreInput(iless));
		
	}

 


	 
	
	
	
	
}
